      "predictiveBackGestureEnabled": false,
      "package": "com.luannzeiro.mottumobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "be60496d-23cf-4a59-845e-57b2c5af042a"
      }
    }
  }
